            <id>user_role</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
    </transition>
    <transition>
        <id>task</id>
        <x>0</x>
        <y>0</y>
        <label>Task</label>
    </transition>
</document>