    <defaultRole>false</defaultRole>
    <!-- ROLES -->
    <role>
        <id>user_role</id>
        <name>user role</name>
    </role>
    <!-- TRANSITIONS -->
    <transition>
        <id>user_task</id>
        <x>0</x>
        <y>0</y>
        <label>User Task</label>
