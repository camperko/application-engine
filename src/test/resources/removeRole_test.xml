<?xml version="1.0" encoding="UTF-8"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://modeler.netgrif.com/petriflow_schema.xsd">
    <id>main</id>
    <initials>ORG</initials>
    <icon>business</icon>
    <defaultRole>false</defaultRole>

    <caseName>Nová organizácia</caseName>
    <!-- ROLES -->
    <role>
        <id>admin</id>
        <title>admin</title>
        <event type="assign">
            <id>1</id>
            <actions phase="pre">
                <action>
                    assignRole("manager")
                </action>
            </actions>
        </event>
        <event type="cancel">
            <id>1</id>
            <actions phase="pre">
                <action>
                    removeRole("manager")
                </action>
            </actions>
        </event>
    </role>
    <role>
        <id>manager</id>
        <title>manager</title>
    </role>
</document>