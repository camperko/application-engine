<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../main/resources/petriNets/petriflow_schema.xsd">
    <id>static_dataset_test</id>
    <title>Static dataset test</title>
    <initials>SDT</initials>
    <!-- ROLES -->
    <data type="text">
        <id>static_text</id>
        <title>Static text</title>
        <init>static value</init>
        <static>true</static>
    </data>
    <data type="file">
        <id>static_file</id>
        <title>Static file</title>
        <static>true</static>
    </data>
    <data type="text">
        <id>regular_text</id>
        <title>Regular text</title>
        <init>regular value</init>
    </data>
    <data type="file">
        <id>regular_file</id>
        <title>Regular file</title>
    </data>

    <data type="number">
        <id>2000</id>
        <title>Static var arc value</title>
        <init>5</init>
        <static>true</static>
    </data>
    <data type="number">
        <id>3000</id>
        <title>Var arc value</title>
        <init>10</init>
    </data>

    <transition>
        <id>transition</id>
        <x>200</x>
        <y>200</y>
        <label>Transition</label>
    </transition>

    <place>
        <id>place1</id>
        <x>150</x>
        <y>200</y>
        <label>Place 1</label>
        <tokens>1</tokens>
        <static>false</static>
    </place>
    <place>
        <id>place2</id>
        <x>350</x>
        <y>100</y>
        <label>Place 2</label>
        <tokens>0</tokens>
        <static>false</static>
    </place>

      <place>
        <id>place3</id>
        <x>350</x>
        <y>300</y>
        <label>Place 3</label>
        <tokens>0</tokens>
        <static>false</static>
    </place>

    <arc>
        <id>a1</id>
        <type>regular</type>
        <sourceId>place1</sourceId>
        <destinationId>transition</destinationId>
        <multiplicity>1</multiplicity>
    </arc>
    <arc>
        <id>a2</id>
        <type>variable</type>
        <sourceId>transition</sourceId>
        <destinationId>place2</destinationId>
        <multiplicity>2000</multiplicity>
    </arc>
    <arc>
        <id>a3</id>
        <type>variable</type>
        <sourceId>transition</sourceId>
        <destinationId>place3</destinationId>
        <multiplicity>3000</multiplicity>
    </arc>
</document>
